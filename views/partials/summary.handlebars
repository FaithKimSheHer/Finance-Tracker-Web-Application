<div id="summary">
    <h2>Account Summary</h2>
    <table id="transactionTable">
        <thead>
            <tr>
                <th>Category</th>
                <th>Amount</th>
                <th>Transaction Date</th>
            </tr>
        </thead>
        <tbody id="recentTrans">
            {{#each summary}}
            <tr>
                <td>{{category}}</td>
                <td>{{amount}}</td>
                <td>{{dateOfTransaction}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
    <script src="../public/js/summary.js"></script>
</div>